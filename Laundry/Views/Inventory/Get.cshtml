@model IEnumerable<Laundry.Models.Inventory>

@{
    ViewBag.Title = "Get";
}

<h2>Inventories</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Inventory_Type)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Inventory_Type)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Inventory_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Inventory_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Inventory_ID })
        </td>
    </tr>
}

</table>
